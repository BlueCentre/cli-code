# CLI-CODE Environment Variables
# Copy this file to .env and uncomment/modify the settings you want to use

# API Keys and URLs
# CLI_CODE_GOOGLE_API_KEY=your_google_api_key_here
# CLI_CODE_OLLAMA_API_URL=http://localhost:11434/v1

# Default Provider (gemini or ollama)
# CLI_CODE_DEFAULT_PROVIDER=ollama

# Default Model Names
# CLI_CODE_DEFAULT_MODEL=gemini-2.5-pro-exp-03-25
# CLI_CODE_OLLAMA_DEFAULT_MODEL=llama3.2:latest

# Logging Level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
# LOG_LEVEL=INFO 